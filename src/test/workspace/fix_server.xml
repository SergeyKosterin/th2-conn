<serviceDescription>
    <type>generic:FIX_Server</type>
    <name>fix_server</name>
    <serviceHandlerClassName>com.exactpro.sf.services.CollectorServiceHandler</serviceHandlerClassName>
    <fixServerSettings>
        <autoStartable>true</autoStartable>
        <dictionaryName>generic:FIX_5_0_GENERIC</dictionaryName>
        <expectedTimeOfStarting>2000</expectedTimeOfStarting>
        <invertStoredMessageTypes>false</invertStoredMessageTypes>
        <performDump>false</performDump>
        <persistMessages>true</persistMessages>
        <waitingTimeBeforeStarting>0</waitingTimeBeforeStarting>
        <allowUnknownMsgFields>false</allowUnknownMsgFields>
        <beginString>FIXT.1.1</beginString>
        <checkLatency>false</checkLatency>
        <defaultApplVerID>9</defaultApplVerID>
        <duplicateTagsAllowed>false</duplicateTagsAllowed>
        <endTime>00:00:00</endTime>
        <fileStorePath>store/fix/sessions</fileStorePath>
        <logHeartbeats>false</logHeartbeats>
        <maxLatency>120</maxLatency>
        <microsecondsInTimeStampFields>false</microsecondsInTimeStampFields>
        <millisecondsInTimeStampFields>true</millisecondsInTimeStampFields>
        <orderingFields>false</orderingFields>
        <receiveLimit>0</receiveLimit>
        <rejectInvalidMessage>true</rejectInvalidMessage>
        <resetOnDisconnect>false</resetOnDisconnect>
        <resetOnLogout>false</resetOnLogout>
        <senderCompID>server</senderCompID>
        <startTime>00:00:00</startTime>
        <targetCompID>client</targetCompID>
        <useLocalTime>false</useLocalTime>
        <useSSL>false</useSSL>
        <validateFieldsHaveValues>true</validateFieldsHaveValues>
        <validateFieldsOutOfOrder>true</validateFieldsOutOfOrder>
        <validateFieldsOutOfRange>true</validateFieldsOutOfRange>
        <validateSequenceNumbers>true</validateSequenceNumbers>
        <validateUserDefinedFields>true</validateUserDefinedFields>
        <applicationClassName>com.exactpro.sf.services.fix.DemoServerApplication</applicationClassName>
        <forceDisconnectByDispose>false</forceDisconnectByDispose>
        <keepMessagesInMemory>true</keepMessagesInMemory>
        <resetOnLogon>false</resetOnLogon>
        <socketAcceptPort>9900</socketAcceptPort>
        <useDataDictionary>false</useDataDictionary>
    </fixServerSettings>
    <variables/>
</serviceDescription>
